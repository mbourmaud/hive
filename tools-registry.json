  "tools": {
    "glab": {
      "name": "GitLab CLI",
      "description": "Command-line interface for GitLab",
      "category": "vcs",
      "install": "binary",
      "url_amd64": "https://gitlab.com/gitlab-org/cli/-/releases/v1.50.0/downloads/glab_1.50.0_linux_amd64.tar.gz",
      "url_arm64": "https://gitlab.com/gitlab-org/cli/-/releases/v1.50.0/downloads/glab_1.50.0_linux_arm64.tar.gz",
      "extract": "tar",
      "binary": "bin/glab",
      "verify": "glab --version"
    },
    "psql": {
      "name": "PostgreSQL Client",
      "description": "Command-line interface for PostgreSQL",
      "category": "database",
      "install": "apt",
      "packages": ["postgresql-client"],
      "verify": "psql --version"
    },
    "mongosh": {
      "name": "MongoDB Shell",
      "description": "Modern MongoDB shell",
      "category": "database",
      "install": "deb",
      "url_amd64": "https://downloads.mongodb.com/compass/mongodb-mongosh_2.1.1_amd64.deb",
      "url_arm64": "https://downloads.mongodb.com/compass/mongodb-mongosh_2.1.1_arm64.deb",
      "verify": "mongosh --version"
    },
    "kubectl": {
      "name": "Kubernetes CLI",
      "description": "Command-line tool for Kubernetes",
      "category": "cloud",
      "install": "binary",
      "url_amd64": "https://dl.k8s.io/release/v1.29.0/bin/linux/amd64/kubectl",
      "url_arm64": "https://dl.k8s.io/release/v1.29.0/bin/linux/arm64/kubectl",
      "verify": "kubectl version --client"
    },
    "helm": {
      "name": "Helm",
      "description": "Kubernetes package manager",
      "category": "cloud",
      "install": "binary",
      "url_amd64": "https://get.helm.sh/helm-v3.14.0-linux-amd64.tar.gz",
      "url_arm64": "https://get.helm.sh/helm-v3.14.0-linux-arm64.tar.gz",
      "extract": "tar",
      "binary": "linux-amd64/helm",
      "binary_arm64": "linux-arm64/helm",
      "verify": "helm version --short"
    },
    "aws": {
      "name": "AWS CLI",
      "description": "Amazon Web Services command-line interface",
      "category": "cloud",
      "install": "script",
      "script_amd64": "curl -s 'https://awscli.amazonaws.com/awscli-exe-linux-x86_64.zip' -o /tmp/awscliv2.zip && unzip -q /tmp/awscliv2.zip -d /tmp && sudo /tmp/aws/install && rm -rf /tmp/aws*",
      "script_arm64": "curl -s 'https://awscli.amazonaws.com/awscli-exe-linux-aarch64.zip' -o /tmp/awscliv2.zip && unzip -q /tmp/awscliv2.zip -d /tmp && sudo /tmp/aws/install && rm -rf /tmp/aws*",
      "verify": "aws --version"
    },
    "az": {
      "name": "Azure CLI",
      "description": "Microsoft Azure command-line interface",
      "category": "cloud",
      "install": "script",
      "script": "curl -sL https://aka.ms/InstallAzureCLIDeb | sudo bash",
      "verify": "az version"
    },
    "mysql": {
      "name": "MySQL Client",
      "description": "Command-line interface for MySQL",
      "category": "database",
      "install": "apt",
      "packages": ["default-mysql-client"],
      "verify": "mysql --version"
    },
    "redis-cli": {
      "name": "Redis CLI",
      "description": "Command-line interface for Redis",
      "category": "database",
      "install": "apt",
      "packages": ["redis-tools"],
      "verify": "redis-cli --version"
    },
    "terraform": {
      "name": "Terraform",
      "description": "Infrastructure as Code tool",
      "category": "cloud",
      "install": "binary",
      "url_amd64": "https://releases.hashicorp.com/terraform/1.7.0/terraform_1.7.0_linux_amd64.zip",
      "url_arm64": "https://releases.hashicorp.com/terraform/1.7.0/terraform_1.7.0_linux_arm64.zip",
      "extract": "zip",
      "binary": "terraform",
      "verify": "terraform version"
    },
    "flyctl": {
      "name": "Fly.io CLI",
      "description": "Command-line interface for Fly.io",
      "category": "cloud",
      "install": "script",
      "script": "curl -L https://fly.io/install.sh | sh && sudo ln -sf /home/agent/.fly/bin/flyctl /usr/local/bin/fly",
      "verify": "fly version"
    },
    "heroku": {
      "name": "Heroku CLI",
      "description": "Command-line interface for Heroku",
      "category": "cloud",
      "install": "npm",
      "package": "heroku",
      "verify": "heroku --version"
    },
    "vercel": {
      "name": "Vercel CLI",
      "description": "Command-line interface for Vercel",
      "category": "cloud",
      "install": "npm",
      "package": "vercel",
      "verify": "vercel --version"
    },
    "netlify": {
      "name": "Netlify CLI",
      "description": "Command-line interface for Netlify",
      "category": "cloud",
      "install": "npm",
      "package": "netlify-cli",
      "verify": "netlify --version"
    },
    "snow": {
      "name": "ServiceNow CLI",
      "description": "Command-line interface for ServiceNow",
